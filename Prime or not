#include <stdio.h>
int main() {
int n;
printf ("enter n : ");
scanf ("%d", &n);
int a=0;

for (int i=2;i<=n-1;i++){
    if (n%i==0){
        a=1;
    }
}
     if (n==1) {
        printf ("NOT a Prime Number, NOT a Composite Number ");
    }

else if (a==0) {
    printf ("Prime Number input");
}

else{
    printf ("Composite Number input");
}


return 0;
}
